  "semanticHighlighting": true,
  "semanticTokenColors": {
    "macro": "#503a00",
    "namespace": "#aeaeae",
    "class": "#003781",
    "interface": "#4d2f7f",
    "function": "#00694c",
    "member": "#00694c",
    "variable": "#9a8b00",
    "variable.readonly": "#525252",
    "variable.readonly.defaultLibrary": "#525252"
  }
